{
	"name": "polyglot-python",
	"version": "1.0.10-SNAPSHOT",
	"description": "Autogenerated python action",
	"private": true,
	"scripts": {
		"build": "polyglotpkg --env vro",
		"clean": "run-script-os",
		"clean:win32": "rmdir /S /Q out dist tmp || echo Nothing to clean",
		"clean:default": "rm -Rf out dist tmp",
		"test": "echo \"Error: no test specified\""
	},
	"keywords": [
		"polyglot",
		"package",
		"vro",
		"python"
	],
	"author": "VMware WWCoE",
	"license": "VMware Confidential",
	"devDependencies": {
		"run-script-os": "^1.1.6"
	},
	"platform": {
		"runtime": "python:3.7",
		"action": "polyglotpython",
		"tags": [],
		"entrypoint": "handler.handler",
		"base": "out"
	},
	"vro": {
		"module": "com.vmware.pscoe.templates.buildtoolsforvmwareariasamples",
		"inputs": {
			"vraHostConfiguration": "CompositeType(host:string,port:number,refreshToken:string):VRAHostConfiguration",
			"vmId": "string",
			"tags": "Array/CompositeType(key:string,value:string):VraTag"
		},
		"outputType": "Array/Properties"
	},
	"files": [
		"out",
		"!package.json"
	],
	"dependencies": {}
}
